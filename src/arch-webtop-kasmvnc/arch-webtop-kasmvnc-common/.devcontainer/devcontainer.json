{
  "image": "ghcr.io/zyrakq/arch-devcontainer-images/arch-webtop-kasmvnc:latest",
  "features": {
    "ghcr.io/zyrakq/arch-devcontainer-features/pacman-mirror:1": {
      "mode": "reflector"
    },
    "ghcr.io/bartventer/arch-devcontainer-features/common-utils:1": {
      "username": "abc",
      "additionalPackages": "base-devel",
      "installZsh": true,
      "installOhMyZsh": true,
      "configureZshAsDefaultShell": true
    }
  },
  "runArgs": [
    "--shm-size=1gb"
  ],
  "remoteUser": "abc",
  "remoteEnv": {
    "HOME": "/config"
  }
}
